release: MAIN.ASM DRAWING.OBJ GAME.OBJ VIDEO.OBJ RAND.OBJ
	ML /W3 /WX DRAWING.OBJ GAME.OBJ RAND.OBJ VIDEO.OBJ MAIN.ASM /Fe MAIN.EXE
debug: GAME.ASM DRAWING_D.OBJ GAME_D.OBJ RAND_D.OBJ VIDEO_D.OBJ
	ML /Zi /W3 /WX DRAWING_D.OBJ GAME_D.OBJ RAND_D.OBJ VIDEO_D.OBJ MAIN.ASM /Fe MAIN_D.EXE
	
VIDEO.OBJ: VIDEO.ASM
	ML /c /W3 /WX /FoVIDEO.OBJ /Ta VIDEO.ASM
	
VIDEO_D.OBJ: VIDEO.ASM
	ML /c /Zi /W3 /WX /FoVIDEO_D.OBJ /Ta VIDEO.ASM
	
RAND.OBJ: RAND.ASM
	ML /c /W3 /WX /FoRAND.OBJ /Ta RAND.ASM
	
RAND_D.OBJ: RAND.ASM
	ML /c /Zi /W3 /WX /FoRAND_D.OBJ /Ta RAND.ASM
	
DRAWING.OBJ: DRAWING.ASM
	ML /c /W3 /WX /FoDRAWING.OBJ /Ta DRAWING.ASM

DRAWING_D.OBJ: DRAWING.ASM
	ML /c /Zi /W3 /WX /FoDRAWING_D.OBJ /Ta DRAWING.ASM
	
GAME.OBJ: GAME.ASM
	ML /c /W3 /WX /FoGAME.OBJ /Ta GAME.ASM

GAME_D.OBJ: GAME.ASM
	ML /c /Zi /W3 /WX /FoGAME_D.OBJ /Ta GAME.ASM
	
	
clean:
	del *.EXE
	del *.OBJ
	del *.BAK
cv:
	cv /s MAIN_D.EXE
